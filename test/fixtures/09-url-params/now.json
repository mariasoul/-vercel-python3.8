{
  "version": 2,
  "builds": [{ "src": "*.py", "use": "@vercel/python3.8" }],
  "routes": [{ "src": "/another", "dest": "custom.py" }],
  "probes": [
    { "path": "/?hello=/", "mustContain": "path:  query: {'hello': '/'}" },
    {
      "path": "/another?hello=/",
      "mustContain": "path: another query: {'hello': '/'}"
    }
  ]
}
