{
  "version": 2,
  "builds": [{ "src": "api/hello.py", "use": "@vercel/python3.8" }],
  "routes": [{ "src": "/", "dest": "/api/hello.py" }],
  "probes": [
    {
      "path": "/",
      "method": "POST",
      "body": {
        "name": "RANDOMNESS_PLACEHOLDER"
      },
      "mustContain": "{\"name\":\"RANDOMNESS_PLACEHOLDER\"}"
    }
  ]
}
